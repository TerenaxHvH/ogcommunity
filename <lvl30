function lastCheck(a){
  // Old accounts (ID 1-751): 14/01/2026
  // New batch accounts (ID 752+): 20/01/2026
  const id = Number(a?.account_id) || 0;
  if(LAST_CHECK_OVERRIDES[id]) return LAST_CHECK_OVERRIDES[id];
  return id >= 1088 ? '21/01/2026' : (id >= 752 ? '20/01/2026' : '14/01/2026');
}


function daysBetween(d1, d2){
  const ms = 24*60*60*1000;
  return Math.floor((d2.getTime() - d1.getTime()) / ms);
}

function rankScore(rankStr){
  const s = (rankStr || "").toString().trim();
  if(!s || /unranked/i.test(s)) return 0;

  const tierOrder = {
    iron: 1, bronze: 2, silver: 3, gold: 4, platinum: 5,
    emerald: 6, diamond: 7, master: 8, grandmaster: 9, challenger: 10
  };

  const tierMatch = s.match(/(Iron|Bronze|Silver|Gold|Platinum|Emerald|Diamond|Master|Grandmaster|Challenger)/i);
  if(!tierMatch) return 0;
  const tier = tierMatch[1].toLowerCase();
  const tierIndex = tierOrder[tier] || 0;

  let divisionScore = 0;
  const divMatch = s.match(/(IV|III|II|I|4|3|2|1)/i);
  if(divMatch) {
    const dv = divMatch[1].toUpperCase();
    const map = { IV: 1, III: 2, II: 3, I: 4, "4": 1, "3": 2, "2": 3, "1": 4 };
    divisionScore = map[dv] || 0;
  } else {
    divisionScore = 5;
  }

  const lpMatch = s.match(/\((\d+)\s*LP\)/i);
  const lp = lpMatch ? parseInt(lpMatch[1],10) : 0;

  return tierIndex*1000 + divisionScore*100 + lp;
}



function clamp(n, min, max){
  return Math.max(min, Math.min(max, n));
}

function tierWeight(rankStr){
  const s = (rankStr || '').toString().trim();
  if(!s || /unranked/i.test(s)) return 0;

  const tierMatch = s.match(/(Iron|Bronze|Silver|Gold|Platinum|Emerald|Diamond|Master|Grandmaster|Challenger)/i);
  if(!tierMatch) return 0;
  const tier = tierMatch[1].toLowerCase();

  const baseMap = {
    iron: 0.06,
    bronze: 0.12,
    silver: 0.20,
    gold: 0.40,
    platinum: 0.58,
    emerald: 0.70,
    diamond: 0.88,
    master: 1.05,
    grandmaster: 1.15,
    challenger: 1.25
  };

  let base = baseMap[tier] ?? 0;

  // small division bonus
  const divMatch = s.match(/(IV|III|II|I|4|3|2|1)/i);
  if(divMatch){
    const dv = divMatch[1].toUpperCase();
    const map = { IV:0.00, III:0.02, II:0.04, I:0.06, '4':0.00, '3':0.02, '2':0.04, '1':0.06 };
    base += (map[dv] ?? 0);
  }

  // tiny LP bonus
  const lpMatch = s.match(/\((\d+)\s*LP\)/i);
  if(lpMatch){
    const lp = Math.min(100, parseInt(lpMatch[1],10) || 0);
    base += (lp/100) * 0.03;
  }

  return base;
}


// --- Skin rarity lists for Est. Price rules ---
const ULTIMATE_SKINS = ['pulsefire ezreal', 'spirit guard udyr', 'dj sona', 'elementalist lux', 'soul fighter samira', 'k/da all out seraphine', 'gun goddess miss fortune', 'arcane fractured jinx'];
const LEGENDARY_FLOOR_SKINS = ['primordian aatrox', 'spirit blossom ahri', 'spirit blossom springs ahri', 'star guardian ahri', 'star guardian akali', 'chosen of the wolf ambessa', 'blackfrost anivia', 'annie in wonderland', 'high noon ashe', 'project: ashe', 'spirit blossom ashe', 'porcelain protector aurelion sol', 'space groove blitz & crank', 'zombie brand', 'battle academia caitlyn', 'pulsefire caitlyn', "battlecast prime cho'gath", "gentleman cho'gath", 'ice toboggan corki', 'red baron corki', 'dunkmaster darius', 'god-king darius', 'corporate mundo', 'true damage ekko', 'coven evelynn', 'battle academia ezreal', 'porcelain protector ezreal', 'gatekeeper galio', 'god-king garen', 'sentinel graves', 'alien invader heimerdinger', 'dragon trainer heimerdinger', 'mythmaker irelia', 'spirit blossom irelia', 'forecast janna', 'mecha kingdoms jax', 'arcane survivor jayce', 'dark cosmic jhin', 'mythmaker jhin', 'star guardian jinx', "star guardian kai'sa", 'faerie queen karma', 'spirit blossom karma', 'battle queen katarina', 'aether wing kayle', 'odyssey kayn', 'god fist lee sin', 'heavenscale lee sin', 'storm dragon lee sin', 'high noon leona', 'lunar eclipse leona', 'solar eclipse leona', 'high noon lucian', 'cosmic lux', 'dark cosmic lux', 'inkshadow master yi', 'project: yi', 'battle bunny miss fortune', 'project: mordekaiser', 'coven morgana', 'coven nami', 'infernal nasus', 'eternum nocturne', 'nunu & willump bot', 'brolaf', 'empyrean pyke', 'project: pyke', 'redeemed star guardian rakan', 'star guardian rakan', 'project: renekton', 'broken covenant riven', 'dawnbringer riven', 'super galaxy rumble', 'high noon senna', 'winterblessed senna', 'battle dove seraphine', 'spirit blossom sett', 'cat-in-the-box shaco', 'mecha zero sion', 'solar eclipse sivir', 'battlecast alpha skarner', 'psyops sona', 'dawnbringer soraka', 'immortal journey soraka', 'nightbringer soraka', 'dark star sylas', 'omega squad teemo', 'space groove teemo', 'dark star thresh', 'spirit blossom thresh', 'dragon trainer tristana', 'firefighter tristana', 'demonblade tryndamere', 'the magnificent twisted fate', 'empyrean varus', 'project: vayne', 'sentinel vayne', 'final boss veigar', 'fright night veigar', 'arcane brawler vi', 'soul fighter viego', 'arcane savior viktor', 'blood lord vladimir', 'battle academia xayah', 'redeemed star guardian xayah', 'star guardian xayah', 'dream dragon yasuo', 'nightbringer yasuo', 'truth dragon yasuo', 'dawnbringer yone', 'high noon yone', 'debonair zed', 'galaxy slayer zed'];
const SKIN_FLOOR_EXCLUDED_REGIONS = new Set(['las','lan','oce','ru']);

const ULTIMATE_MATCH_SUBSTRINGS = ULTIMATE_SKINS.map(s => (s || '').toString().toLowerCase());
function normSkinName(x){
  return (x || '').toString().toLowerCase().replace(/\s+/g,' ').trim();
}

function ultimateCount(a){
  if(typeof a.ultimate_count === 'number') return a.ultimate_count;
  const skins = Array.isArray(a.skins) ? a.skins : [];
  const seen = new Set();
  for(const raw of skins){
    const n = normSkinName(raw);
    for(const u of ULTIMATE_MATCH_SUBSTRINGS){
      if(!u) continue;
      // Use substring matching so variants like 'K/DA ALL OUT Seraphine Indie' are counted
      if(n.includes(u)) seen.add(u);
    }
  }
  a.ultimate_count = seen.size;
  return a.ultimate_count;
}
// --- Rare skins list for Rare Skins filter (based on the provided rare-skin lists) ---
// We use grouped patterns so spelling variants (Judgement/Judgment) count as ONE.
const RARE_SKIN_GROUPS = [
  { key: 'pax twisted fate', patterns: ['pax twisted fate'] },
  { key: 'pax jax', patterns: ['pax jax'] },
  { key: 'pax sivir', patterns: ['pax sivir', 'neo pax sivir'] },
  { key: 'king rammus', patterns: ['king rammus'] },
  { key: 'silver kayle', patterns: ['silver kayle'] },
  { key: 'black alistar', patterns: ['black alistar'] },
  { key: 'young ryze', patterns: ['young ryze', 'human ryze'] },
  { key: 'rusty blitzcrank', patterns: ['rusty blitzcrank'] },
  { key: 'ufo corki', patterns: ['ufo corki'] },
  { key: 'riot squad singed', patterns: ['riot squad singed', 'riot singed'] },
  { key: 'victorious jarvan iv', patterns: ['victorious jarvan iv'] },
  { key: 'judgement kayle', patterns: ['judgement kayle', 'judgment kayle'] },
  { key: 'triumphant ryze', patterns: ['triumphant ryze'] },
];

const RARE_GROUPS = RARE_SKIN_GROUPS.map(g => ({
  key: g.key,
  patterns: (g.patterns || []).map(p => (p || '').toString().toLowerCase())
}));

function rareCount(a){
  if(typeof a.rare_count === 'number') return a.rare_count;
  const skins = Array.isArray(a.skins) ? a.skins : [];
  const found = new Set();

  for(const raw of skins){
    const n = normSkinName(raw);
    for(const g of RARE_GROUPS){
      if(found.has(g.key)) continue;
      for(const p of g.patterns){
        if(!p) continue;
        if(n.includes(p)) { found.add(g.key); break; }
      }
    }
  }

  a.rare_count = found.size;
  return a.rare_count;
}


// Legendary skins count (for Legendary Skins filter)
// Uses exact-match against the provided Legendary list (case-insensitive)
const LEGENDARY_SET = new Set(LEGENDARY_FLOOR_SKINS.map(normSkinName));

function legendaryCount(a){
  if(typeof a.legendary_count === 'number') return a.legendary_count;
  const skins = Array.isArray(a.skins) ? a.skins : [];
  let count = 0;
  for(const raw of skins){
    const n = normSkinName(raw);
    if(LEGENDARY_SET.has(n)) count++;
  }
  a.legendary_count = count;
  return a.legendary_count;
}


// --- Skin grouping for the account details modal (Enter) ---
function isUltimateSkinName(raw){
  const n = normSkinName(raw);
  for(const u of ULTIMATE_MATCH_SUBSTRINGS){
    if(u && n.includes(u)) return true;
  }
  return false;
}

function isRareSkinName(raw){
  const n = normSkinName(raw);
  for(const g of RARE_GROUPS){
    for(const p of g.patterns){
      if(p && n.includes(p)) return true;
    }
  }
  return false;
}

function isLegendarySkinName(raw){
  const n = normSkinName(raw);
  return LEGENDARY_SET.has(n);
}

// Epic skins (1350 RP) are not directly available in the TXT export.
// We approximate this by classifying common reward/exclusive skins as "Rest",
// and treating the remaining normal skins as "Epic".
const REST_KEYWORDS = [
  'prestige','hextech','ashen','mythic','victorious','championship','worlds','msi','conqueror',
  'pax ','neo pax','riot ','judgement','judgment','silver kayle','black alistar','king rammus',
  'rusty','ufo ','triumphant','young ryze','human ryze','three honors','honor',
  'skt t1','fnatic','tpa ','ssg ','fpx','edg','ig ','dwg','gen.g','g2 ','rogue','mad lions','c9','tsm'
];

 // --- Selection (quick buy total) ---
const DATA_BY_ID = new Map(DATA.map(a => [Number(a.account_id)||0, a]));
const selectedIds = new Set();
let lastSelIndex = null;
let lastVisibleIds = [];

function updateSelectionUI(){
  const count = selectedIds.size;
  let total = 0;
  for(const id of selectedIds){
    const a = DATA_BY_ID.get(id);
    if(a) total += (Number(getPrice(a)) || 0);
  }
  const elCount = document.getElementById('selCount');
  const elTotal = document.getElementById('selTotal');
  if(elCount) elCount.textContent = String(count);
  if(elTotal) elTotal.textContent = fmtPrice(total);

  const pill = document.getElementById('pillSelected');
  if(pill) pill.classList.toggle('hasSel', count > 0);
}

function syncSelectAllCheckbox(){
  const chk = document.getElementById('chkAllVisible');
  if(!chk) return;
  const cbs = Array.from(document.querySelectorAll('#tbody input.rowSel'));
  if(cbs.length === 0){
    chk.checked = false;
    chk.indeterminate = false;
    chk.disabled = true;
    return;
  }
  chk.disabled = false;
  const checkedCount = cbs.reduce((acc,cb)=> acc + (cb.checked ? 1 : 0), 0);
  chk.checked = checkedCount === cbs.length;
  chk.indeterminate = checkedCount > 0 && checkedCount < cbs.length;
}

function copySelection(){
  const ids = Array.from(selectedIds).sort((a,b)=>a-b);
  let total = 0;
  for(const id of ids){
    const a = DATA_BY_ID.get(id);
    if(a) total += (Number(getPrice(a)) || 0);
  }
  const msg = `Selected (${ids.length}) | Total: ${fmtPrice(total)}\nIDs: ${ids.join(', ')}`;
  copyText(msg);
}

function clearSelection(){
  selectedIds.clear();
  lastSelIndex = null;
  document.querySelectorAll('#tbody input.rowSel').forEach(cb=>{
    cb.checked = false;
    cb.closest('tr')?.classList.remove('rowSelected');
  });
  updateSelectionUI();
  syncSelectAllCheckbox();
}

function isRestSkinName(raw){
  const n = normSkinName(raw);
  for(const k of REST_KEYWORDS){
    if(k && n.includes(k)) return true;
  }
  return false;
}

// --- Extra cosmetic grouping helpers for the account details modal (Chromas / Ward Skins / Rest cosmetics) ---
function isChromaName(raw){
  const n = normSkinName(raw);
  return /\bchroma\b/i.test(n);
}

// Ward skins (approx): detect the standalone word "ward" (but not "warden", etc.)
function isWardSkinName(raw){
  const n = normSkinName(raw);
  return /\bward\b/i.test(n);
}

// Non-champion-skin cosmetics that can appear inside the exported "skins" list
// Examples: Summoner Icons, Emotes, TFT Little Legends/Arenas/Boons, etc.
const NON_SKIN_KEYWORDS = [
  ' icon', ' emote', 'little legend', 'tft ', ' arena', 'boom', 'chibi',
  'illustration icon', 'summoner icon'
];
function isNonSkinCosmeticName(raw){
  const n = normSkinName(raw);
  // Explicit keywords
  for(const k of NON_SKIN_KEYWORDS){
    if(k && n.includes(k)) return true;
  }
  // Some icons are named without the word "icon" (e.g. "Season 2024")
  if(/^season\s+\d{4}$/i.test(n)) return true;
  return false;
}

// Chroma value ordering (heuristic): bundle-exclusives like Ruby are usually the most valuable.
const CHROMA_VALUE_ORDER = [
  'ruby','obsidian','pearl','emerald','sapphire','tanzanite','citrine','amethyst',
  'rose quartz','catseye','aquamarine','turquoise','granite','night','dawn','day',
  'event','exclusive'
];
function chromaValueScore(name){
  const n = normSkinName(name);
  for(let i=0;i<CHROMA_VALUE_ORDER.length;i++){
    if(n.includes(CHROMA_VALUE_ORDER[i])) return (CHROMA_VALUE_ORDER.length - i);
  }
  // numeric / default variants get lowest score
  return 0;
}

// Find which champion a cosmetic belongs to (best-effort)
function detectChampionFromText(raw, champions){
  const n = normSkinName(raw);
  const list = Array.isArray(champions) ? champions : [];
  for(const c of list){
    const cn = normSkinName(c);
    if(!cn) continue;
    // allow "nunu & willump" and similar
    if(n.includes(cn)) return c;
  }
  return null;
}



function groupedSkinsTextForModal(a){
  const items = Array.isArray(a.skins) ? a.skins.slice() : [];

  const explicitChromas = Array.isArray(a.chromas) ? a.chromas.slice() : [];
  const explicitWards = Array.isArray(a.ward_skins) ? a.ward_skins.slice() : [];


  const rare = [];
  const ult = [];
  const leg = [];
  const epic = [];
  const others = [];      // all other champion skins (incl. exclusives / rewards)
  const wardSkins = [];
  const restCosmetics = [];

  // Chromas grouped by champion
  const chromaByChamp = new Map(); // champName -> string[]
  const chromaUnknown = [];

  // keep unique names (case-insensitive)
  const seen = new Set();

  for(const raw of items){
    const key = normSkinName(raw);
    if(!key || seen.has(key)) continue;
    seen.add(key);

    // 1) Rest cosmetics (icons/emotes/TFT/etc.) should NOT be mixed with champion skins
    if(isNonSkinCosmeticName(raw)){
      restCosmetics.push(raw);
      continue;
    }

    // 2) Chromas
    if(explicitChromas.length===0 && isChromaName(raw)){
      const champ = detectChampionFromText(raw, a && a.champions);
      if(champ){
        if(!chromaByChamp.has(champ)) chromaByChamp.set(champ, []);
        chromaByChamp.get(champ).push(raw);
      } else {
        chromaUnknown.push(raw);
      }
      continue;
    }

    // 3) Ward skins
    if(explicitWards.length===0 && isWardSkinName(raw)){
      wardSkins.push(raw);
      continue;
    }

    // 4) Champion skins (grouped)
    // Ensure ONLY champion skins go into these buckets.
    // If we can't detect a champion name, treat it as "Rest" (icons/emotes/etc.).
    const detectedChamp = detectChampionFromText(raw, a && a.champions);
    if(!detectedChamp){
      restCosmetics.push(raw);
      continue;
    }

    if(isRareSkinName(raw)) rare.push(raw);
    else if(isUltimateSkinName(raw)) ult.push(raw);
    else if(isLegendarySkinName(raw)) leg.push(raw);
    else if(isRestSkinName(raw)) others.push(raw);
    else epic.push(raw);
  }  // If Chromas/Ward Skins are provided from the TXT export, prefer them over detection
  if(explicitChromas.length){
    const seenC = new Set();
    for(const raw of explicitChromas){
      const key = normSkinName(raw);
      if(!key || seenC.has(key)) continue;
      seenC.add(key);

      const champ = detectChampionFromText(raw, a && a.champions);
      if(champ){
        if(!chromaByChamp.has(champ)) chromaByChamp.set(champ, []);
        chromaByChamp.get(champ).push(raw);
      }else{
        chromaUnknown.push(raw);
      }
    }
  }

  if(explicitWards.length){
    const seenW = new Set(wardSkins.map(normSkinName));
    for(const raw of explicitWards){
      const key = normSkinName(raw);
      if(!key || seenW.has(key)) continue;
      seenW.add(key);
      wardSkins.push(raw);
    }
  }


  const sorter = (x,y) => x.localeCompare(y, undefined, { sensitivity:'base' });

  rare.sort(sorter);
  ult.sort(sorter);
  leg.sort(sorter);
  epic.sort(sorter);
  others.sort(sorter);
  wardSkins.sort(sorter);
  restCosmetics.sort(sorter);

  // Format chromas: champions alphabetically, each champion's chromas by (value desc, name asc)
  const champNames = Array.from(chromaByChamp.keys()).sort(sorter);
  const chromaLines = [];

  for(const champ of champNames){
    const arr = chromaByChamp.get(champ) || [];
    arr.sort((a,b)=>{
      const va = chromaValueScore(a);
      const vb = chromaValueScore(b);
      if(vb !== va) return vb - va; // higher value first
      return sorter(a,b);
    });

    chromaLines.push(`${champ}:`);
    for(const c of arr) chromaLines.push(`  - ${c}`);
    chromaLines.push(''); // spacing line
  }

  if(chromaUnknown.length){
    chromaUnknown.sort((a,b)=>{
      const va = chromaValueScore(a);
      const vb = chromaValueScore(b);
      if(vb !== va) return vb - va;
      return sorter(a,b);
    });
    chromaLines.push(`Unknown Champion:`);
    for(const c of chromaUnknown) chromaLines.push(`  - ${c}`);
  }

  function section(title, arr){
    // Show title with count: "Rare Skins (3):"
    const clean = (title || '').toString().replace(/:\s*$/, '');
    return `${clean} (${arr.length}):\n${arr.length ? arr.join('\n') : '-'}`;
  }

  function sectionChromas(){
    const chromaTotal =
      Array.from(chromaByChamp.values()).reduce((s,a)=>s + ((a && a.length) ? a.length : 0), 0) +
      (chromaUnknown ? chromaUnknown.length : 0);

    const body = chromaLines.length ? chromaLines.join('\n').trimEnd() : '-';
    return `Chromas (${chromaTotal}):\n${body}`;
  }

  return [
    section('Rare Skins:', rare),
    section('Ultimate Skins:', ult),
    section('Legendary Skins:', leg),
    section('Epic Skins:', epic),
    section('Others:', others),
    section('Rest:', restCosmetics),
    sectionChromas(),
    section('Ward Skins:', wardSkins),
  ].join('\n\n');
}


// =============================
// Loot Items grouping + sorting
// =============================
let __championSetCache = null;
function getChampionNameSet(){
  if(__championSetCache) return __championSetCache;
  __championSetCache = new Set();
  try{
    for(const a of (DATA || [])){
      for(const c of (a.champions || [])){
        if(c) __championSetCache.add(c);
      }
    }
  }catch(e){}
  return __championSetCache;
}

function parseLootEntry(s){
  if(!s) return null;
  const str = String(s).trim();
  if(!str) return null;
  const m = str.match(/^(.*?)(?:\s*-\s*(\d+))\s*$/);
  if(m){
    return { name: m[1].trim(), qty: Math.max(1, parseInt(m[2], 10) || 1), raw: str };
  }
  return { name: str, qty: 1, raw: str };
}

function isPermanentLoot(name){
  const n = String(name || '');
  return /\(Permanent\)/i.test(n) || /\bPermanent\b/i.test(n);
}

function lootValueScore(name, cat){
  const n = String(name||'').toLowerCase();
  let s = 0;

  // Generic high-value signals
  if(n.includes('mythic')) s += 50;
  if(n.includes('prestige')) s += 35;
  if(n.includes('worlds')) s += 30;
  if(n.includes('msi')) s += 28;
  if(n.includes('masterwork')) s += 40;
  if(n.includes('hextech')) s += 25;
  if(n.includes('prime')) s += 20;
  if(n.includes('event')) s += 18;

  if(cat === 'chests'){
    if(n.includes('masterwork')) s += 80;
    if(n.includes('hextech')) s += 60;
  }

  if(cat === 'capsules'){
    if(n.includes('capsule')) s += 25;
    if(n.includes('orb')) s += 22;
  }

  if(cat === 'series'){
    // Prefer lower series number (often older / more valuable)
    const mm = n.match(/series\s*(\d+)/i);
    if(mm){
      const k = parseInt(mm[1],10);
      if(!isNaN(k)) s += Math.max(0, 40 - k);
    }
  }

  if(cat === 'emotes'){
    if(isPermanentLoot(name)) s += 30;
    if(n.includes('mystery')) s += 15;
  }

  // Honor / basic loot tends to be lower
  if(n.includes('honor')) s -= 10;
  if(n.includes('blue essence')) s -= 15;

  return s;
}

function lootCat(entry){
  const name = String(entry?.name || '');
  const n = name.toLowerCase();

  // Capsules / Orbs
  if(n.includes('capsule') || n.includes('orb') || n.includes('grab bag') || n.includes('bag')) return 'capsules';

  // Chests
  if(n.includes('chest')) return 'chests';

  // Keys
  if(n.includes('key fragment')) return 'keyFragments';
  // "Key - 2"
  if(n === 'key' || (/\bkey\b/.test(n) && !n.includes('keyboard'))) return 'keys';

  // Emotes
  if(n.includes('emote')) return 'emotes';

  // Series (Eternals / Series X)
  if(n.includes('eternal') && n.includes('series')) return 'series';
  if(n.includes('series') && (n.includes('eternal') || n.includes('eternals'))) return 'series';
  if(/\bseries\s*\d+\b/i.test(name)) return 'series';


// Tokens
  if(n.includes('token')) return 'tokens';

  // Currencies
  if(n.includes('blue essence') || n === 'be') return 'blueEssence';
  if(n.includes('orange essence') || n === 'oe') return 'orangeEssence';
  if(/\brp\b/.test(n) || n.includes('riot point') || n.includes('riot points') || (n.includes('currency') && n.includes('rp'))) return 'rpCurrency';
  // Extra currencies / tickets
  if(n.includes('prestige point') || n.includes('prestige points')) return 'prestigePoints';
  if(n.includes('mythic essence') || n.includes('currency_mythic') || n.includes('currency mythic')) return 'mythicEssence';
  if(n.includes('gemstone') || n.includes('gemstones')) return 'gemstones';
  if(n.includes('clash ticket') || n.includes('clash tickets')) return 'clashTickets';


  // Ward Skin Shards
  if((n.includes('ward') && n.includes('shard')) || (n.includes('ward') && n.includes('(shard)'))) return 'wardShards';

    // Permanents (e.g. "Cho'Gath (Permanent)") should not go to Remaining
  if(isPermanentLoot(name)){
    const cleanedPerm = name
      .replace(/\s*\(Permanent\)\s*/ig,'')
      .replace(/\bPermanent\b/ig,'')
      .trim();
    const champSet = getChampionNameSet();
    if(champSet && champSet.has(cleanedPerm)) return 'champShards';
    if(n.includes('ward')) return 'wardShards';
    return 'skinShards';
  }

// Shards (Skin / Champion)
  if(n.includes('shard') || n.includes('(shard)')){
    const cleaned = name
      .replace(/\s*\(Permanent\)\s*/ig,'')
      .replace(/\s*\(Shard\)\s*/ig,'')
      .replace(/\bSkin Shard\b/ig,'')
      .replace(/\bChampion Shard\b/ig,'')
      .trim();

    const champSet = getChampionNameSet();
    if(champSet && champSet.has(cleaned)) return 'champShards';
    return 'skinShards';
  }

  return 'rest';
}

function sortLootList(list, cat){
  const arr = (list || []).slice();
  arr.sort((a,b)=>{
    // Permanent first for shard-like categories
    if(cat === 'skinShards' || cat === 'champShards' || cat === 'wardShards'){
      const ap = isPermanentLoot(a.name), bp = isPermanentLoot(b.name);
      if(ap !== bp) return ap ? -1 : 1;
    }
    // Value score (desc) then name (asc)
    const sa = lootValueScore(a.name, cat);
    const sb = lootValueScore(b.name, cat);
    if(sa !== sb) return sb - sa;
    return String(a.name||'').localeCompare(String(b.name||''));
  });
  return arr;
}

function fmtLootLine(e){
  const q = e.qty ?? 1;
  return q > 1 ? `${e.name} - ${q}` : `${e.name} - 1`;
}

function fmtKeysLine(keys, frags){
  const k = Number(keys)||0;
  const f = Number(frags)||0;
  const kTxt = (k === 1) ? '1 key' : `${k} keys`;
  const fTxt = (f === 1) ? '1 key fragment' : `${f} key fragments`;
  return `${kTxt} and ${fTxt}`;
}

function sectionBlock(title, total, lines){
  const hdr = `${title} (${total}):`;
  if(!lines || lines.length === 0) return `${hdr}\n-`;
  return `${hdr}\n${lines.join('\n')}`;
}

function lootItemsTextForModal(a){
  const itemsRaw = (a && a.loot_items) ? a.loot_items : [];
  if(!itemsRaw || itemsRaw.length === 0) return '-';

  const parsed = [];
  for(const s of itemsRaw){
    const e = parseLootEntry(s);
    if(e) parsed.push(e);
  }
  if(parsed.length === 0) return '-';

  const buckets = {
    capsules: [],
    chests: [],
    keys: [],
    keyFragments: [],
    skinShards: [],
    champShards: [],
    wardShards: [],
    series: [],
    tokens: [],
    emotes: [],
    blueEssence: [],
    orangeEssence: [],
    rpCurrency: [],
    prestigePoints: [],
    mythicEssence: [],
    gemstones: [],
    clashTickets: [],
    rest: [],
  };

  for(const e of parsed){
    const c = lootCat(e);
    if(buckets[c]) buckets[c].push(e);
    else buckets.rest.push(e);
  }

  // Totals
  const sumQty = (arr)=>arr.reduce((acc,x)=>acc + (x.qty||1), 0);

  // Keys summary
  const keyTotal = sumQty(buckets.keys);
  const fragTotal = sumQty(buckets.keyFragments);

  // Sorted lists
  const capsules = sortLootList(buckets.capsules, 'capsules');
  const chests = sortLootList(buckets.chests, 'chests');
  const skinShards = sortLootList(buckets.skinShards, 'skinShards');
  const champShards = sortLootList(buckets.champShards, 'champShards');
  const wardShards = sortLootList(buckets.wardShards, 'wardShards');
  const series = sortLootList(buckets.series, 'series');
  const tokens = sortLootList(buckets.tokens, 'tokens');
  const emotes = sortLootList(buckets.emotes, 'emotes');
  const rest = sortLootList(buckets.rest, 'rest');

  const out = [];

  // Capsules
  out.push(sectionBlock('Capsules', sumQty(capsules), capsules.map(x=>`- ${fmtLootLine(x)}`)));

  // Chests
// Aggregate CHEST_* IDs into a single "CHEST" line, and combine regular chests as well.
function normalizeChestLabel(name){
  const n = String(name || '').trim();
  if(!n) return '';
  const low = n.toLowerCase();
  if(low.includes('regular chest')) return 'Regular Chest';
  if(/^CHEST_/i.test(n) || /^CHEST$/i.test(n)) return 'CHEST';
  return n;
}
const chestAggMap = new Map(); // label -> qty
for(const e of chests){
  const label = normalizeChestLabel(e.name);
  const q = (e.qty || 1);
  if(!label) continue;
  chestAggMap.set(label, (chestAggMap.get(label) || 0) + q);
}
const chestAggList = Array.from(chestAggMap.entries()).map(([name, qty])=>({name, qty}));
chestAggList.sort((a,b)=>{
  // higher qty first, then value score (masterwork/hextech), then name
  const dq = (b.qty||1) - (a.qty||1);
  if(dq) return dq;
  const sv = lootValueScore(b.name, 'chests') - lootValueScore(a.name, 'chests');
  if(sv) return sv;
  return String(a.name).localeCompare(String(b.name), undefined, {sensitivity:'base'});
});
out.push(sectionBlock('Chests', sumQty(chests), chestAggList.map(x=>`- ${fmtLootLine(x)}`)));

  // Keys (single-line summary)
  out.push(`Keys: ${fmtKeysLine(keyTotal, fragTotal)}`);


  // Skin shards (perm first)
  out.push(sectionBlock('Skin Shards', sumQty(skinShards), skinShards.map(x=>`- ${fmtLootLine(x)}`)));

  // Champion shards (perm first)
  out.push(sectionBlock('Champion Shards', sumQty(champShards), champShards.map(x=>`- ${fmtLootLine(x)}`)));

  // Ward skin shards (perm first)
  out.push(sectionBlock('Ward Skin Shards', sumQty(wardShards), wardShards.map(x=>`- ${fmtLootLine(x)}`)));

  // Series
  out.push(sectionBlock('Series', sumQty(series), series.map(x=>`- ${fmtLootLine(x)}`)));


  // Tokens
  out.push(sectionBlock('Tokens', sumQty(tokens), tokens.map(x=>`- ${fmtLootLine(x)}`)));

  // Emotes
  out.push(sectionBlock('Emotes', sumQty(emotes), emotes.map(x=>`- ${fmtLootLine(x)}`)));

  // Currencies (shown separately; never in Remaining)
  const beFromLoot = sumQty(buckets.blueEssence || []);
  const oeFromLoot = sumQty(buckets.orangeEssence || []);
  const rpFromLoot = sumQty(buckets.rpCurrency || []);

  const beAmt = (beFromLoot > 0) ? beFromLoot : (Number(a?.blue_essence ?? a?.blueEssence ?? 0) || 0);
  const oeAmt = (oeFromLoot > 0) ? oeFromLoot : (Number(a?.orange_essence ?? a?.orangeEssence ?? 0) || 0);
  const rpAmt = (rpFromLoot > 0) ? rpFromLoot : (Number(a?.rp ?? a?.riot_points ?? a?.currency_rp ?? 0) || 0);

  // Always show currency lines (0 if none)
  out.push(`Blue Essence: ${beAmt || 0}`);
  out.push(`Orange Essence: ${oeAmt || 0}`);
  out.push(`RP: ${rpAmt || 0}`);

  // Extra currencies / tickets (always show 0 if none)
  const ppFromLoot = sumQty(buckets.prestigePoints || []);
  const meFromLoot = sumQty(buckets.mythicEssence || []);
  const gsFromLoot = sumQty(buckets.gemstones || []);
  const ctFromLoot = sumQty(buckets.clashTickets || []);

  const ppAmt = (ppFromLoot > 0) ? ppFromLoot : (Number(a?.prestige_points ?? a?.prestigePoints ?? 0) || 0);
  const meAmt = (meFromLoot > 0) ? meFromLoot : (Number(a?.mythic_essence ?? a?.mythicEssence ?? a?.mythic ?? 0) || 0);
  const gsAmt = (gsFromLoot > 0) ? gsFromLoot : (Number(a?.gemstones ?? a?.gemstone ?? 0) || 0);
  const ctAmt = (ctFromLoot > 0) ? ctFromLoot : (Number(a?.clash_tickets ?? a?.clashTickets ?? 0) || 0);

  out.push(`Prestige Points: ${ppAmt || 0}`);
  out.push(`Mythic: ${meAmt || 0}`);
  out.push(`Gemstones: ${gsAmt || 0}`);
  out.push(`Clash Tickets: ${ctAmt || 0}`);


  // Remaining
  const remaining = rest;
  if(remaining.length){
    out.push(sectionBlock('Remaining', sumQty(remaining), remaining.map(x=>`- ${fmtLootLine(x)}`)));
  }

  return out.join('\n\n');
}

function getMaxLegendary(){
  let mx = 0;
  for(const a of DATA){
    const c = legendaryCount(a);
    if(c > mx) mx = c;
  }
  return mx;
}


