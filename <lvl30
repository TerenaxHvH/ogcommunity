function computePrice(a){
  const id = Number(a?.account_id) || 0;
  if(FIXED_PRICE_OVERRIDES[id] != null) return FIXED_PRICE_OVERRIDES[id];
  // currencies
  const rp = Number(a.rp) || 0;
  const be = Number(a.be) || 0;
  const oe = Number(a.oe) || 0;
  const me = Number(a.me) || 0;

  const skins = Array.isArray(a.skins) ? a.skins : [];
  const skinsCount = Number(a.skins_count ?? skins.length) || 0;

  const sLower = skins.map(x => (x || '').toString().toLowerCase());
  // very high-value (ultimate) skins
  let ultimate = ultimateCount(a);

  // rare skins count
  const rareHits = rareCount(a);

  // count key legendary skins for floor rules (2+ gives a minimum price bump)
  let legendaryHits = legendaryCount(a);

  // premium keywords (prestige/hextech/mythic/etc.)
  const premiumKeys = ['prestige','hextech','ashen','mythic','victorious','championship','worlds','t1','msi','conqueror'];
  let premium = 0;
  for(const s of sLower){
    for(const k of premiumKeys){
      if(s.includes(k)) { premium++; break; }
    }
  }

  // points (pricing emphasizes champion skins; currencies have only a small impact; level has no impact)
  const skinPoints = skinsCount * 0.010 + premium * 0.020 + ultimate * 0.120;

  const currencyPoints =
    clamp(rp, 0, 500) * 0.00035 +
    (clamp(be, 0, 200000) / 200000) * 0.08 +
    (clamp(oe, 0, 2000) / 2000) * 0.04 +
    (clamp(me, 0, 250) / 250) * 0.10;

  // rank points (small influence)
  const soloW = tierWeight(a.solo_rank);
  const flexW = tierWeight(a.flex_rank_last_season);
  let rankPoints = soloW * 0.22 + flexW * 0.12;
  if(soloW >= 0.40) rankPoints += 0.03;
  if(flexW >= 0.40) rankPoints += 0.02;

  // champions count has only a tiny influence
  const champsCount = Number(a.champions_count ?? (Array.isArray(a.champions) ? a.champions.length : 0)) || 0;
  const champPoints = (clamp(champsCount, 0, 200) / 200) * 0.06;

  // inactivity bonus (inactive accounts get a bump)
  const d = parseDMY(a.last_game_date);
  const lc = parseDMY(a.last_check) || LAST_CHECKED;
  const days = d ? clamp(daysBetween(d, lc), 0, 1500) : 365;
  let inactivityFactor = 0.95 + (clamp(days, 0, 365) / 365) * 0.25; // 0.95 .. 1.20
  if(days < 15) inactivityFactor *= 0.92; // very active -> slightly less
  if(days >= 180) inactivityFactor *= 1.05;

  // penalties: restrictions / penalty time reduce value
  const rr = Number(a.ranked_restriction_games_remaining) || 0;
  const ng = Number(a.remaining_normal_games_to_play) || 0;
  const pm = Number(a.penalty_minutes) || 0;
  const penalty = (rr > 0 ? 0.25 : 0) + (ng > 0 ? 0.10 : 0) + (pm > 0 ? 0.15 : 0);

  let score = (skinPoints + currencyPoints + rankPoints + champPoints - penalty) * inactivityFactor;

  // normalize -> $0.10 .. $5.00
  const maxScore = 10.5;
  const n = clamp(score / maxScore, 0, 1);
  const price = 0.10 + n * 3.90;

  // --- Price rules requested by customer ---
  let adjusted = price;

  // Parse last game date once
  const lg = parseDMY(a.last_game_date);
  // Region-based supply discount (LAS/LAN are much cheaper; ME/RU/OCE cheaper)
  // Applied AFTER the recent-date clamp so these regions can still end up lower.
  const reg = norm(a.region);
  if(reg === 'las' || reg === 'lan'){
    adjusted *= 0.35; // much lower (lots of supply)
  } else if(reg === 'me' || reg === 'ru' || reg === 'oce'){
    adjusted *= 0.60; // lower (lots of supply)
  }

  // 4) Make sure the $0.10 minimum is actually used:
  // Nudge very cheap accounts down by one $0.05 step (before final stepping).
  if(adjusted < 0.50) adjusted -= 0.05;

  // 4.5) Active accounts tier pricing (<= 30 days inactive, based on champs + BE + skins; Level has NO influence)
  // This creates more "logical" small differences for active accounts in $0.05 steps.
  if(lg){
    const inactiveDaysActive = clamp(daysBetween(lg, lc), 0, 5000);
    if(inactiveDaysActive <= 30){
      // Base tiers from your rules:
      // 0.10, 0.15, 0.20, 0.25, 0.30, 0.35 ... and continue upwards by +0.05 with higher stats
      const tiers = [
        { c: 0,   be: 0,      s: 0,   p: 0.10 },   // <100 champs, <80k BE, <70 skins
        { c: 80,  be: 50000,  s: 50,  p: 0.15 },
        { c: 100, be: 80000,  s: 70,  p: 0.20 },
        { c: 120, be: 120000, s: 90,  p: 0.25 },
        { c: 130, be: 120000, s: 100, p: 0.30 },
        { c: 130, be: 120000, s: 110, p: 0.35 },
        { c: 130, be: 135000, s: 130, p: 0.40 },  // next step (continuation)
      ];

      let base = 0.10;
      let last = tiers[0];
      for(const t of tiers){
        if(champsCount >= t.c && be >= t.be && skinsCount >= t.s){
          base = t.p;
          last = t;
        }
      }

      // Continue the pattern beyond the last tier:
      // Every time stats increase enough (roughly together), price goes +$0.05
      const extraC  = Math.floor(Math.max(0, champsCount - last.c) / 20);
      const extraBE = Math.floor(Math.max(0, be - last.be) / 50000);
      const extraS  = Math.floor(Math.max(0, skinsCount - last.s) / 50);
      const extra   = Math.min(extraC, extraBE, extraS);

      let forcedActive = base + extra * 0.05;

      // Apply the same region supply discount to the forced active price
      if(reg === 'las' || reg === 'lan'){
        forcedActive *= 0.35;
      } else if(reg === 'me' || reg === 'ru' || reg === 'oce'){
        forcedActive *= 0.60;
      }

      // Set active tier price (other floors/caps can still apply after this)
      adjusted = forcedActive;
    }
  }



// 5) Skin-based minimum price floors
// Not applied for LAS/LAN/OCE/RU (as requested)
if(!SKIN_FLOOR_EXCLUDED_REGIONS.has(reg)){
  const rankedReady = isRankedReady(a);
  if(!rankedReady){
    // NOT ranked ready accounts: adjusted floors requested
    // Ultimate minimum:
    // 1 ultimate -> min $0.10
    // 3 ultimate -> min $0.20
    // each extra 2 ultimate -> +$0.05 min
    let minUlt = 0;
    if(ultimate >= 1) minUlt = 0.10;
    if(ultimate >= 3){
      minUlt = Math.max(minUlt, 0.20 + Math.floor((ultimate - 3) / 2) * 0.05);
    }
    if(minUlt > 0) adjusted = Math.max(adjusted, minUlt);

    // Legendary minimum: 6+ legendary -> min $0.15
    if(legendaryHits >= 6){
      adjusted = Math.max(adjusted, 0.15);
    }
  } else {
    // Ranked ready accounts keep the normal skin floors
    if(ultimate >= 1){
      const minUlt = 0.10 + (ultimate - 1) * 0.05;
      adjusted = Math.max(adjusted, minUlt);
    }
    if(legendaryHits >= 4){
      adjusted = Math.max(adjusted, 0.15);
    }
  }
}
// 5b) Inactive 30+ tier pricing (specific stats bands)
  // Applies for accounts that are inactive 30+ days relative to their own Last Check.
  // Note: inactive tiers are applied based on your rules.
  if(lg){
    const lcTier = parseDMY(a.last_check) || LAST_CHECKED;
    const inactiveDaysTier = clamp(daysBetween(lg, lcTier), 0, 5000);
    if(inactiveDaysTier >= 30){
      const hasUlt = (ultimate >= 1);
      const hasRare = (rareHits >= 1);
      const isEUWorTR = (reg === 'euw' || reg === 'tr');

      const inRange = (v, lo, hi) => v >= lo && v <= hi;
      let forced = null;

      // Highest tiers first (7 -> 1) to resolve overlaps correctly
      if(inRange(champsCount,105,135) && inRange(be,80000,135000) && inRange(skinsCount,50,85)){
        forced = 0.40;
        if(isEUWorTR){
          if(hasRare) forced = 0.50;
          else if(hasUlt) forced = 0.45;
        }
      } else if(inRange(champsCount,100,125) && inRange(be,75000,110000) && inRange(skinsCount,40,75)){
        forced = 0.35;
        if(isEUWorTR){
          if(hasRare) forced = 0.45;
          else if(hasUlt) forced = 0.40;
        }
      } else if(inRange(champsCount,85,110) && inRange(be,65000,95000) && inRange(skinsCount,35,65)){
        forced = 0.30;
        if(isEUWorTR){
          if(hasRare) forced = 0.40;
          else if(hasUlt) forced = 0.35;
        }
      } else if(inRange(champsCount,75,90) && inRange(be,50000,80000) && inRange(skinsCount,25,50)){
        forced = 0.25;
        if(isEUWorTR){
          if(hasRare) forced = 0.35;
          else if(hasUlt) forced = 0.30;
        }
      } else if(inRange(champsCount,55,75) && inRange(be,35000,60000) && inRange(skinsCount,20,40)){
        forced = 0.20;
        if(isEUWorTR){
          if(hasRare) forced = 0.30;
          else if(hasUlt) forced = 0.25;
        }
      } else if(inRange(champsCount,40,60) && inRange(be,25000,40000) && inRange(skinsCount,15,35)){
        forced = 0.15;
        if(isEUWorTR){
          if(hasRare) forced = 0.25;
          else if(hasUlt) forced = 0.20;
        }
      } else if(champsCount < 40 && be < 30000 && skinsCount < 25){
        forced = 0.10;
        if(isEUWorTR){
          if(hasRare) forced = 0.20;
          else if(hasUlt) forced = 0.15;
        }
      }

      if(forced !== null){
        adjusted = forced;
      }
    }
  }



  // 5c) Per-skin price boosts (requested):
  // Rare skins: +$0.10 for each rare skin
  // Ultimate skins: +$0.05 for each ultimate skin
  {
    const rankedReadyBoost = isRankedReady(a);
    // Boost rules:
    // - NOT ranked ready: Rare +$0.05 per rare skin; Ultimate +$0.05 per 2 ultimate skins
    // - Ranked ready: keep stronger boosts (Rare +$0.10 each; Ultimate +$0.05 each)
    if(!rankedReadyBoost){
      adjusted += (rareHits * 0.05) + (Math.floor(ultimate / 2) * 0.05);
    } else {
      adjusted += (rareHits * 0.10) + (ultimate * 0.05);
    }
  }

  // 6) Active accounts (30 days or more recent since their own Last Check) maximum caps:
  // - Max $1.50 for active accounts (<= 30 days inactive)
  // - If they have 1+ Rare Skins OR 160+ skins => max $2.00
  // - Except LAN/LAS and ME/RU/OCE always max $0.80
  if(lg){
    const lcCap = parseDMY(a.last_check) || LAST_CHECKED;
    const inactiveDays = clamp(daysBetween(lg, lcCap), 0, 5000);
    if(inactiveDays <= 30){
      const hasRare = rareCount(a) >= 1;
      const has160Skins = skinsCount >= 160;
      if(reg === 'las' || reg === 'lan' || reg === 'me' || reg === 'ru' || reg === 'oce'){
        adjusted = Math.min(adjusted, 0.80);
      } else {
        const cap = (hasRare || has160Skins) ? 2.00 : 1.50;
        adjusted = Math.min(adjusted, cap);
      }
    }
  }


  // 6) Extra boost for INACTIVE 60+ days accounts (except LAN/LAS/OCE/VM/VN/RU/TW/SG/ME), starting from $0.40+
  // Goal: allow strong inactive accounts to scale up toward ~$3.00 without changing cheap accounts
  const INACTIVE_BOOST_EXCLUDED_REGIONS = new Set(['lan','las','oce','vm','vn','ru','tw','sg','me']);
  if(days >= 60 && adjusted >= 0.40 && !INACTIVE_BOOST_EXCLUDED_REGIONS.has(reg)){
    const lvl = Number(a.level ?? a.summoner_level) || 0;
    const leg = legendaryCount(a);

    // Normalize stats (0..1)
    const skinsN = clamp(skinsCount, 0, 350) / 350;
    const legN   = clamp(leg, 0, 40) / 40;
    const ultN   = clamp(ultimate, 0, 5) / 5;
    const rareN  = clamp(rareHits, 0, 8) / 8;
    const champN = clamp(champsCount, 0, 200) / 200;
    const lvlN   = clamp(lvl, 0, 500) / 500;

    // Stats-driven multiplier (1.00 .. 3.00)
    let statsMul = 1 + clamp(
      skinsN*0.90 +
      legN*0.90 +
      ultN*0.70 +
      rareN*0.50 +
      champN*0.30 +
      lvlN*0.30,
      0, 2.0
    );

    // Slight extra for being more inactive (60d..300d -> up to +20%)
    const inactivityMul = 1 + clamp((days - 60) / 240, 0, 1) * 0.20;

    adjusted = Math.min(adjusted * statsMul * inactivityMul, 3.00);
  }

  // Accounts always start from $0.10 minimum
  adjusted = Math.max(adjusted, 0.10);

  // Keep $0.05 increments (round to nearest 5 cents)
  const mul = 20; // 1 / 0.05
  let stepped = (adjusted < 0.30)
    ? (Math.floor((adjusted + 1e-9) * mul) / mul)
    : (Math.round((adjusted + 1e-9) * mul) / mul);
  // avoid floating point artifacts like 0.30000000000000004
  stepped = Math.round(stepped * 100) / 100;
  return clamp(stepped, 0.10, 4.00);
}

function getPrice(a){
  return '-';
}


function fmtPrice(p){
  return '-';
}


function isRankedReady(a){
  const rr = Number(a.ranked_restriction_games_remaining);
  const ng = Number(a.remaining_normal_games_to_play);
  const pm = Number(a.penalty_minutes);
  if([rr,ng,pm].some(x => Number.isNaN(x))) return false;
  return rr === 0 && ng === 0 && pm === 0;
}

function activityPass(a){
  const opt = state.filters.activity;
  if(opt === "All") return true;

  const d = parseDMY(a.last_game_date);
  if(!d) return false;
  const lc = parseDMY(lastCheck(a));
  if(!lc) return false;
  const diff = daysBetween(d, lc);

  // Support year-based inactivity shortcuts
  // e.g. 'Inactive 3y+' means last game date is at least 3 years older than LAST_CHECKED
  const y = opt.match(/Inactive\s+(\d+)y\+/i);
  if(y){
    const years = parseInt(y[1], 10);
    if(!Number.isNaN(years)) {
      const threshold = new Date(lc);
      threshold.setFullYear(threshold.getFullYear() - years);
      return d <= threshold;
    }
  }


  if(opt === "Active Only") return diff < 30;

  const m = opt.match(/Inactive\s+(\d+)\+/i);
  if(m){
    const x = parseInt(m[1],10);
    return diff >= x;
  }
  return true;
}

function matchesList(list, q){
  if(!q) return true;
  const target = norm(q);
  const arr = list || [];
  return arr.some(x => norm(x).includes(target));
}

// AND filter: account must contain ALL selected items (exact match, case-insensitive)
function listHasAll(list, requiredArr){
  const req = (requiredArr || []).map(norm).filter(Boolean);
  if(req.length === 0) return true;
  const set = new Set((list || []).map(norm));
  return req.every(r => set.has(r));
}

function matchesSearch(a, q){
  if(!q) return true;
  const s = norm(q);
  const blob = [
    a.region,
    a.last_game_date,
    a.solo_rank,
    a.flex_rank_last_season,
    a.level,
    a.rp,
    a.be,
    a.oe,
    a.me,
    a.champions_count,
    a.skins_count,
    a.honor_level,
    fmtPrice(getPrice(a)),
    isRankedReady(a) ? 'ranked ready' : 'not ready'
  ].join(' | ');
  return norm(blob).includes(s);
}

function getFiltered(){
  const f = state.filters;

  return DATA.filter(a => {
    if(f.region !== "All" && a.region !== f.region) return false;
    if(!activityPass(a)) return false;
    if(!matchesSearch(a, f.search.trim())) return false;
    // multi-select filters (AND logic)
    if(!listHasAll(a.champions, f.championsSelected)) return false;
    if(!listHasAll(a.skins, f.skinsSelected)) return false;
    if(f.ultimateMin && ultimateCount(a) < f.ultimateMin) return false;
    if(f.rareMin && rareCount(a) < f.rareMin) return false;
    if(f.legendaryMin && legendaryCount(a) < f.legendaryMin) return false;
    if(state.rankedReadyOnly && !isRankedReady(a)) return false;
    return true;
  });
}

function sortData(arr){
  if(!state.sortKey) return arr;
  const k = state.sortKey;
  const dir = state.sortDir;
  const copy = arr.slice();

  copy.sort((a,b)=>{
    let va = a[k];
    let vb = b[k];

    if(k === "ranked_ready"){
      return ((isRankedReady(a)?1:0) - (isRankedReady(b)?1:0)) * dir;
    }

    if(k === "solo_rank" || k === "flex_rank_last_season"){
      return (rankScore(va) - rankScore(vb)) * dir;
    }

    if(k === "price"){
      return (getPrice(a) - getPrice(b)) * dir;
    }

    if(k === "last_game_date"){
      const da = parseDMY(va);
      const db = parseDMY(vb);
      const ta = da ? da.getTime() : 0;
      const tb = db ? db.getTime() : 0;
      return (ta - tb) * dir;
    }


    if(k === "last_check"){
      const da = parseDMY(lastCheck(a));
      const db = parseDMY(lastCheck(b));
      const ta = da ? da.getTime() : 0;
      const tb = db ? db.getTime() : 0;
      return (ta - tb) * dir;
    }

    const numKeys = ["account_id","level","rp","be","oe","me","champions_count","skins_count","honor_level","price"];
    if(numKeys.includes(k)){
      return ((Number(va)||0) - (Number(vb)||0)) * dir;
    }

    va = (va ?? "").toString();
    vb = (vb ?? "").toString();
    return va.localeCompare(vb) * dir;
  });

  return copy;
}

function escapeHtml(s){
  return (s ?? "").toString()
    .replaceAll("&","&amp;")
    .replaceAll("<","&lt;")
    .replaceAll(">","&gt;")
    .replaceAll('"',"&quot;")
    .replaceAll("'","&#039;");
}

function fmtHonor(h){
  if (h === null || h === undefined || h === "" || Number.isNaN(h)) return "-";
  const n = Number(h);
  if (!Number.isFinite(n)) return "-";
  return String(n);
}

function setPills(total, showing, ready){
  document.getElementById("pillShowing").textContent = `Showing ${showing} / ${total}`;
  document.getElementById("pillReady").textContent = `Ranked Ready: ${ready}`;
}

let currentModalAccount = null;

function openModal(a){
  currentModalAccount = a;

  const rr = isRankedReady(a);
  document.getElementById('modalTitle').textContent = `${(a.account_id!=null?('ID '+a.account_id+' • '):'')}${a.region} • Level ${a.level ?? 0} • ${rr ? 'Ranked Ready ✅' : 'Not Ready ❌'}`;
  document.getElementById('modalSub').textContent = `Last game: ${a.last_game_date || '-'} • Last check: ${lastCheck(a)} • Solo: ${a.solo_rank || 'Unranked'} • Flex (Last Season): ${a.flex_rank_last_season || 'Unranked'}`;

  const info = [
    ['RP', fmtNum(a.rp)],
    ['BE', fmtNum(a.be)],
    ['OE', fmtNum(a.oe)],
    ['ME', fmtNum(a.me)],
    ['Champs', fmtNum(a.champions_count)],
    ['Skins', fmtNum(a.skins_count)],
    ['Honor', fmtHonor(a.honor_level)],
    ['Est. Price', fmtPrice(getPrice(a))],
    ['Ranked restriction games', fmtNum(a.ranked_restriction_games_remaining)],
    ['Normal games needed', fmtNum(a.remaining_normal_games_to_play)],
    ['Penalty (mins)', fmtNum(a.penalty_minutes)],
  ];

  const grid = document.getElementById('infoGrid');
  grid.innerHTML = '';
  for(const [k,v] of info){
    const div = document.createElement('div');
    div.className = 'infoCell';
    div.innerHTML = `<div class="k">${escapeHtml(k)}</div><div class="v">${escapeHtml(v)}</div>`;
    grid.appendChild(div);
  }

  const champs = (a.champions || []).slice().sort((x,y)=>x.localeCompare(y));
  const skins = (a.skins || []).slice().sort((x,y)=>x.localeCompare(y));
  document.getElementById('preChamps').textContent = champs.join('\n') || '-';
  document.getElementById('preSkins').textContent = groupedSkinsTextForModal(a) || '-';
  document.getElementById('preLoot').textContent = lootItemsTextForModal(a);
  document.getElementById('lootTitle').textContent = `Loot Items (${(a.loot_items||[]).length})`;

  document.getElementById('modal').classList.remove('hidden');
}

function closeModal(){
  document.getElementById('modal').classList.add('hidden');
  currentModalAccount = null;
}

async function copyText(t){
  try{
    await navigator.clipboard.writeText(t);
  }catch(e){
    // fallback
    const ta = document.createElement('textarea');
    ta.value = t;
    document.body.appendChild(ta);
    ta.select();
    document.execCommand('copy');
    ta.remove();
  }
}

function render(){
  const filtered = getFiltered();
  const sorted = sortData(filtered);
  lastVisibleIds = sorted.map(a => Number(a.account_id)||0);

  const readyCount = sorted.reduce((acc,a)=>acc + (isRankedReady(a)?1:0), 0);
  setPills(DATA.length, sorted.length, readyCount);

  document.getElementById("btnRankedReady").classList.toggle("primary", state.rankedReadyOnly);

  const tbody = document.getElementById("tbody");
  tbody.innerHTML = "";

  const frag = document.createDocumentFragment();

  for(let i=0;i<sorted.length;i++){
    const a = sorted[i];
    const rr = isRankedReady(a);
    const id = Number(a.account_id) || 0;
    const checked = selectedIds.has(id) ? 'checked' : '';
    const tr = document.createElement("tr");
    if(checked) tr.classList.add("rowSelected");
    tr.innerHTML = `
      <td class="pinL0 selCell"><input type="checkbox" class="rowSel" data-id="${id}" data-idx="${i}" ${checked}></td>
      <td class="pinL1 col-region">${escapeHtml(a.region)}</td>
      <td>${fmtNum(a.account_id)}</td>
      <td>${fmtNum(a.level)}</td>
      <td>${fmtNum(a.rp)}</td>
      <td>${fmtNum(a.be)}</td>
      <td>${fmtNum(a.oe)}</td>
      <td>${fmtNum(a.me)}</td>
      <td>${escapeHtml(a.last_game_date || "")}</td>
            <td>${escapeHtml(lastCheck(a))}</td>
<td>${escapeHtml(a.solo_rank || "Unranked")}</td>
      <td>${escapeHtml(a.flex_rank_last_season || "Unranked")}</td>
      <td>${fmtNum(a.champions_count)}</td>
      <td>${fmtNum(a.skins_count)}</td>
      <td>${fmtHonor(a.honor_level)}</td>
      <td>${fmtPrice(getPrice(a))}</td>
    `;
    const cb = tr.querySelector('input.rowSel');
    if(cb){
      cb.addEventListener('click', (ev)=>{
        ev.stopPropagation();
        const want = cb.checked;

        if(ev.shiftKey && lastSelIndex !== null){
          const start = Math.min(lastSelIndex, i);
          const end = Math.max(lastSelIndex, i);

          // apply to range
          document.querySelectorAll('#tbody input.rowSel').forEach(el=>{
            const j = Number(el.dataset.idx) || 0;
            if(j >= start && j <= end){
              el.checked = want;
              const rid = Number(el.dataset.id) || 0;
              if(rid){
                if(want) selectedIds.add(rid); else selectedIds.delete(rid);
              }
              el.closest('tr')?.classList.toggle('rowSelected', want);
            }
          });
        }else{
          if(id){
            if(want) selectedIds.add(id); else selectedIds.delete(id);
          }
          tr.classList.toggle('rowSelected', want);
        }

        lastSelIndex = i;
        updateSelectionUI();
        syncSelectAllCheckbox();
      });
    }

    tr.addEventListener('click', ()=>openModal(a));
    frag.appendChild(tr);
  }

  tbody.appendChild(frag);
  updateSelectionUI();
  syncSelectAllCheckbox();
}

function initSelects(){
  const selRegion = document.getElementById("selRegion");
  const regions = Array.from(new Set(DATA.map(a => a.region))).sort();
  selRegion.innerHTML = `<option value="All">All</option>` + regions.map(r => `<option value="${r}">${r}</option>`).join("");
  selRegion.value = state.filters.region;

  const selActivity = document.getElementById("selActivity");
  const opts = [
    "All","Active Only",
    "Inactive 30+","Inactive 60+","Inactive 90+","Inactive 120+",
    "Inactive 150+","Inactive 180+","Inactive 210+","Inactive 240+","Inactive 270+",
    "Inactive 300+","Inactive 330+","Inactive 360+","Inactive 440+","Inactive 500+",
    "Inactive 600+","Inactive 720+","Inactive 3y+","Inactive 4y+"
  ];
  selActivity.innerHTML = opts.map(o => `<option value="${o}">${o}</option>`).join("");
  if(!opts.includes(state.filters.activity)) state.filters.activity = "All";
  selActivity.value = state.filters.activity;
}

function initDatalists(){
  const champs = new Set();
  const skins = new Set();

  for(const a of DATA){
    for(const c of (a.champions || [])) champs.add(c);
    for(const s of (a.skins || [])) skins.add(s);
  }

  const dlC = document.getElementById('dlChampion');
  dlC.innerHTML = Array.from(champs).sort((x,y)=>x.localeCompare(y)).map(x => `<option value="${escapeHtml(x)}"></option>`).join('');

  const dlS = document.getElementById('dlSkin');
  // skins can be a lot, but still fine (≈1800 in your dataset)
  dlS.innerHTML = Array.from(skins).sort((x,y)=>x.localeCompare(y)).map(x => `<option value="${escapeHtml(x)}"></option>`).join('');
}

function resetFilters(){
  state.filters = {
    region: "All",
    activity: "All",
    search: "",
    championsSelected: [],
    skinsSelected: [],
    ultimateMin: 0,
    rareMin: 0,
    legendaryMin: 0,
    // legacy single-text filters (kept for backward compatibility)
    champion: "",
    skin: ""
  };
  state.rankedReadyOnly = false;
  state.sortKey = '';
  state.sortDir = 1;

  // sync UI
  document.getElementById('selRegion').value = 'All';
  document.getElementById('selActivity').value = 'All';
  document.getElementById('inpSearch').value = '';
  document.getElementById('inpChampion').value = '';
  document.getElementById('inpSkin').value = '';
  renderMultiTags();
  updateUltimateUI();
  updateRareUI();
  updateLegendaryUI();

  saveState();
  render();
}

function exportCSV(){
  const rows = sortData(getFiltered());
  const header = ['Region','Account ID','Level','RP','BE','OE','ME','LastGame','LastCheck','SoloRank','FlexRankLastSeason','ChampionsCount','SkinsCount','Honor','Est. Price'];

  const lines = [header.join(',')];
  for(const a of rows){
    const vals = [
      a.region,
      a.account_id ?? '',
      a.level ?? 0,
      a.rp ?? 0,
      a.be ?? 0,
      a.oe ?? 0,
      a.me ?? 0,
      a.last_game_date ?? '',
      lastCheck(a),
      a.solo_rank ?? 'Unranked',
      a.flex_rank_last_season ?? 'Unranked',
      a.champions_count ?? 0,
      a.skins_count ?? 0,
      a.honor_level ?? '',
      getPrice(a)
    ].map(v => (v ?? '').toString().replaceAll('"','""'))
     .map(v => /[\n,"]/g.test(v) ? `"${v}"` : v);

    lines.push(vals.join(','));
  }

  const csv = lines.join('\n');
  const blob = new Blob([csv], {type:'text/csv;charset=utf-8'});
  const url = URL.createObjectURL(blob);
  const a = document.createElement('a');
  a.href = url;
  a.download = 'lol_accounts_filtered.csv';
  document.body.appendChild(a);
  a.click();
  a.remove();
  URL.revokeObjectURL(url);
}


function exportTXT(){
  // Export ONLY the accounts selected via the checkbox column
  const selected = Array.from(selectedIds || [])
    .map(id => DATA_BY_ID.get(Number(id) || 0))
    .filter(Boolean);

  const rows = sortData(selected);

  if(rows.length === 0){
    alert('No accounts selected. Please tick the checkbox(es) first.');
    return;
  }

  const totalExported = rows.length;
  const totalPriceAll = rows.reduce((sum,a)=> sum + (Number(getPrice(a)) || 0), 0);

  const pad2 = (n)=> String(n).padStart(2,'0');
  const fmtExportDate = (d)=>{
    const dd = pad2(d.getDate());
    const mm = pad2(d.getMonth()+1);
    const yyyy = d.getFullYear();
    const h = d.getHours();           // no leading zero (matches example)
    const min = pad2(d.getMinutes()); // keep 2 digits
    return `${dd}/${mm}/${yyyy} ${h}:${min}`;
  };

  // TXT export as a table with pipes and dashed separators
  const cols = [
    {label:'Region', get:(a)=>a.region ?? ''},
    {label:'Account ID', get:(a)=>(a.account_id ?? ''), numeric:true},
    {label:'Level', get:(a)=>(a.level ?? 0), numeric:true},
    {label:'RP', get:(a)=>(a.rp ?? 0), numeric:true},
    {label:'BE', get:(a)=>(a.be ?? 0), numeric:true},
    {label:'OE', get:(a)=>(a.oe ?? 0), numeric:true},
    {label:'ME', get:(a)=>(a.me ?? 0), numeric:true},
    {label:'LastGame', get:(a)=>a.last_game_date ?? ''},
    {label:'LastCheck', get:(a)=>lastCheck(a)},
    {label:'SoloRank', get:(a)=>a.solo_rank ?? 'Unranked'},
    {label:'FlexRankLastSeason', get:(a)=>a.flex_rank_last_season ?? 'Unranked'},
    {label:'ChampionsCount', get:(a)=>(a.champions_count ?? 0), numeric:true},
    {label:'SkinsCount', get:(a)=>(a.skins_count ?? 0), numeric:true},
    {label:'Honor', get:(a)=>a.honor_level ?? ''},
    {label:'Est. Price', get:(a)=>fmtPrice(getPrice(a)), numeric:true},
    {label:'Total Accounts', get:()=>'', numeric:true},
    {label:'Total Price', get:()=>'', numeric:true},
    {label:'Date', get:()=>''}
  ];

  const sanitize = (v) => (v ?? '').toString()
    .replaceAll('\t',' ')
    .replaceAll('\n',' ')
    .replaceAll('\r',' ')
    .trim();

  // Build table strings
  const table = rows.map(a => cols.map(c => sanitize(c.get(a))));

  // Add totals row (shown only in export) as the first row
  const exportDate = fmtExportDate(new Date());
  const totalsRow = new Array(cols.length).fill('');
  totalsRow[cols.length-3] = String(totalExported);
  totalsRow[cols.length-2] = fmtPrice(totalPriceAll);
  totalsRow[cols.length-1] = exportDate;
  table.unshift(totalsRow);

  // Calculate max width for each column
  const widths = cols.map((c, i) => {
    let w = c.label.length;
    for(const r of table){
      if(r[i].length > w) w = r[i].length;
    }
    return w;
  });

  const pad = (s, w, alignRight=false) => {
    const str = sanitize(s);
    return alignRight ? str.padStart(w, ' ') : str.padEnd(w, ' ');
  };

  const colSep = ' | ';

  const makeLine = (cells, isHeader=false) => {
    return cells.map((cell, i) => {
      const alignRight = !isHeader && cols[i].numeric === true;
      return pad(cell, widths[i], alignRight);
    }).join(colSep).trimEnd();
  };

  const sepLine = widths.map(w => '-'.repeat(w)).join(colSep).trimEnd();

  const lines = [];
  lines.push(makeLine(cols.map(c => c.label), true));
  lines.push(sepLine);
  for(const r of table){
    lines.push(makeLine(r, false));
    lines.push(sepLine);
  }

  // UTF-8 BOM for Notepad
  const txt = '\ufeff' + lines.join('\r\n');

  const blob = new Blob([txt], {type:'text/plain;charset=utf-8'});
  const url = URL.createObjectURL(blob);
  const a = document.createElement('a');
  a.href = url;
  a.download = 'lol_accounts_filtered.txt';
  document.body.appendChild(a);
  a.click();
  a.remove();
  URL.revokeObjectURL(url);
}


function splitMultiInput(raw){
  return (raw || '')
    .split(/[,;\n]+/g)
    .map(x => x.trim())
    .filter(Boolean);
}

function renderMultiTags(){
  const champWrap = document.getElementById('champTags');
  const skinWrap = document.getElementById('skinTags');
  if(!champWrap || !skinWrap) return;

  const champs = state.filters.championsSelected || [];
  const skins = state.filters.skinsSelected || [];

  champWrap.innerHTML = '';
  for(let i=0;i<champs.length;i++){
    const name = champs[i];
    const el = document.createElement('span');
    el.className = 'tag';
    el.innerHTML = `<span class="tagText">${escapeHtml(name)}</span><button class="tagX" title="Remove">×</button>`;
    el.querySelector('button').addEventListener('click', (e)=>{
      e.stopPropagation();
      removeSelected('champ', i);
    });
    champWrap.appendChild(el);
  }

  skinWrap.innerHTML = '';
  for(let i=0;i<skins.length;i++){
    const name = skins[i];
    const el = document.createElement('span');
    el.className = 'tag';
    el.innerHTML = `<span class="tagText">${escapeHtml(name)}</span><button class="tagX" title="Remove">×</button>`;
    el.querySelector('button').addEventListener('click', (e)=>{
      e.stopPropagation();
      removeSelected('skin', i);
    });
    skinWrap.appendChild(el);
  }
}

function addSelected(kind, raw){
  const parts = splitMultiInput(raw);
  if(parts.length === 0) return;

  const key = (kind === 'champ') ? 'championsSelected' : 'skinsSelected';
  if(!Array.isArray(state.filters[key])) state.filters[key] = [];
  const arr = state.filters[key];

  for(const p of parts){
    if(!arr.some(x => norm(x) === norm(p))) arr.push(p);
  }

  // clear input
  const inp = document.getElementById(kind === 'champ' ? 'inpChampion' : 'inpSkin');
  if(inp) inp.value = '';

  renderMultiTags();
  render();
  saveState();
}

function removeSelected(kind, idx){
  const key = (kind === 'champ') ? 'championsSelected' : 'skinsSelected';
  const arr = state.filters[key] || [];
  arr.splice(idx, 1);
  state.filters[key] = arr;
  renderMultiTags();
  render();
  saveState();
}

function popLastSelected(kind){
  const key = (kind === 'champ') ? 'championsSelected' : 'skinsSelected';
  const arr = state.filters[key] || [];
  if(arr.length === 0) return;
  arr.pop();
  state.filters[key] = arr;
  renderMultiTags();
  render();
  saveState();
}


function updateUltimateUI(){
  const btn = document.getElementById('btnUltimateSkins');
  const menu = document.getElementById('ultimateMenu');
  if(!btn || !menu) return;
  const n = Number(state.filters.ultimateMin||0);
  btn.innerHTML = n ? `Ultimate Skins<br/>(${n}+)` : 'Ultimate Skins';
  menu.querySelectorAll('button[data-min]').forEach(b=>{
    const v = Number(b.getAttribute('data-min'));
    b.classList.toggle('active', v === n);
  });
}

function updateRareUI(){
  const btn = document.getElementById('btnRareSkins');
  const menu = document.getElementById('rareMenu');
  if(!btn || !menu) return;
  const n = Number(state.filters.rareMin||0);
  btn.innerHTML = n ? `Rare Skins<br/>(${n}+)` : 'Rare Skins';
  menu.querySelectorAll('button[data-min]').forEach(b=>{
    const v = Number(b.getAttribute('data-min'));
    b.classList.toggle('active', v === n);
  });
}


function buildLegendaryMenu(){
  const wrap = document.getElementById('legendaryNums');
  const menu = document.getElementById('legendaryMenu');
  if(!wrap || !menu) return;
  const max = Math.max(1, getMaxLegendary());
  let html = '';
  html += `<button class="ultNum clear" data-min="0">Clear</button>`;
  for(let i=1;i<=max;i++){
    html += `<button class="ultNum" data-min="${i}">${i}</button>`;
  }
  wrap.innerHTML = html;
}

function updateLegendaryUI(){
  const btn = document.getElementById('btnLegendarySkins');
  const menu = document.getElementById('legendaryMenu');
  if(!btn || !menu) return;
  const n = Number(state.filters.legendaryMin||0);
  btn.innerHTML = n ? `Legendary Skins<br/>(${n}+)` : 'Legendary Skins';
  menu.querySelectorAll('button[data-min]').forEach(b=>{
    const v = Number(b.getAttribute('data-min'));
    b.classList.toggle('active', v === n);
  });
}

function positionLegendaryMenu(){
  const btn = document.getElementById('btnLegendarySkins');
  const menu = document.getElementById('legendaryMenu');
  if(!btn || !menu || menu.classList.contains('hidden')) return;

  const r = btn.getBoundingClientRect();
  const gap = 10;
  const menuW = Math.min(520, window.innerWidth - 24);
  menu.style.width = menuW + 'px';

  let left = r.left + (r.width/2) - (menuW/2);
  left = Math.max(12, Math.min(window.innerWidth - menuW - 12, left));

  let top = r.bottom + gap;
  const menuH = menu.getBoundingClientRect().height || 160;
  if(top + menuH > window.innerHeight - 12){
    top = Math.max(12, r.top - gap - menuH);
  }

  menu.style.left = left + 'px';
  menu.style.top = top + 'px';
  menu.style.transform = 'none';
}

function hideLegendaryMenu(){
  const menu = document.getElementById('legendaryMenu');
  if(menu) menu.classList.add('hidden');
}

function toggleLegendaryMenu(){
  const menu = document.getElementById('legendaryMenu');
  if(!menu) return;
  menu.classList.toggle('hidden');
  updateLegendaryUI();
  positionLegendaryMenu();
}

function positionRareMenu(){
  const btn = document.getElementById('btnRareSkins');
  const menu = document.getElementById('rareMenu');
  if(!btn || !menu || menu.classList.contains('hidden')) return;

  const r = btn.getBoundingClientRect();
  const gap = 10;
  const menuW = Math.min(420, window.innerWidth - 24);
  menu.style.width = menuW + 'px';

  let left = r.left + (r.width/2) - (menuW/2);
  left = Math.max(12, Math.min(window.innerWidth - menuW - 12, left));

  let top = r.bottom + gap;
  const menuH = menu.getBoundingClientRect().height || 120;
  if(top + menuH > window.innerHeight - 12){
    top = Math.max(12, r.top - gap - menuH);
  }

  menu.style.left = left + 'px';
  menu.style.top = top + 'px';
  menu.style.transform = 'none';
}

function hideRareMenu(){
  const menu = document.getElementById('rareMenu');
  if(menu) menu.classList.add('hidden');
}

function toggleRareMenu(){
  const menu = document.getElementById('rareMenu');
  if(!menu) return;
  menu.classList.toggle('hidden');
  updateRareUI();
  positionRareMenu();
}

function positionUltimateMenu(){
  const btn = document.getElementById('btnUltimateSkins');
  const menu = document.getElementById('ultimateMenu');
  if(!btn || !menu || menu.classList.contains('hidden')) return;

  const r = btn.getBoundingClientRect();
  const gap = 10;
  const menuW = Math.min(380, window.innerWidth - 24);
  menu.style.width = menuW + 'px';

  let left = r.left + (r.width/2) - (menuW/2);
  left = Math.max(12, Math.min(window.innerWidth - menuW - 12, left));

  let top = r.bottom + gap;
  const menuH = menu.getBoundingClientRect().height || 120;
  if(top + menuH > window.innerHeight - 12){
    top = Math.max(12, r.top - gap - menuH);
  }

  menu.style.left = left + 'px';
  menu.style.top = top + 'px';
  menu.style.transform = 'none';
}

function hideUltimateMenu(){
  const menu = document.getElementById('ultimateMenu');
  if(menu) menu.classList.add('hidden');
}

function toggleUltimateMenu(){
  const menu = document.getElementById('ultimateMenu');
  if(!menu) return;
  menu.classList.toggle('hidden');
  updateUltimateUI();
  positionUltimateMenu();
}

function hookControls(){
  document.getElementById("btnTheme").addEventListener("click", toggleTheme);

  document.getElementById("selRegion").addEventListener("change", (e)=>{
    state.filters.region = e.target.value;
    render(); saveState();
  });

  document.getElementById("selActivity").addEventListener("change", (e)=>{
    state.filters.activity = e.target.value;
    render(); saveState();
  });

  document.getElementById("inpSearch").addEventListener("input", (e)=>{
    state.filters.search = e.target.value;
    render(); saveState();
  });
  // Multi-select Champion/Skin filters (press Enter to add)
  const champInp = document.getElementById('inpChampion');
  champInp.addEventListener('keydown', (e)=>{
    if(e.key === 'Enter' || e.key === ','){
      e.preventDefault();
      addSelected('champ', champInp.value);
    }
    if(e.key === 'Backspace' && champInp.value === ''){
      popLastSelected('champ');
    }
  });

  const skinInp = document.getElementById('inpSkin');
  skinInp.addEventListener('keydown', (e)=>{
    if(e.key === 'Enter' || e.key === ','){
      e.preventDefault();
      addSelected('skin', skinInp.value);
    }
    if(e.key === 'Backspace' && skinInp.value === ''){
      popLastSelected('skin');
    }
  });

document.getElementById("btnRankedReady").addEventListener("click", ()=>{
    state.rankedReadyOnly = !state.rankedReadyOnly;
    render(); saveState();
  });

  document.getElementById('btnReset').addEventListener('click', resetFilters);
  document.getElementById('btnExportTxt').addEventListener('click', exportTXT);
    const btnExportCsv = document.getElementById('btnExportCsv');
  if(btnExportCsv) btnExportCsv.addEventListener('click', exportCSV);
// Selection controls
  const btnClearSel = document.getElementById('btnClearSel');
  if(btnClearSel) btnClearSel.addEventListener('click', (e)=>{ e.preventDefault(); clearSelection(); });

  const btnCopySel = document.getElementById('btnCopySel');
  if(btnCopySel) btnCopySel.addEventListener('click', (e)=>{ e.preventDefault(); copySelection(); });

  const chkAll = document.getElementById('chkAllVisible');
  if(chkAll){
    chkAll.addEventListener('click', (e)=>e.stopPropagation());
    chkAll.addEventListener('change', ()=>{
      const want = chkAll.checked;
      document.querySelectorAll('#tbody input.rowSel').forEach(cb=>{
        cb.checked = want;
        const id = Number(cb.dataset.id) || 0;
        if(id){
          if(want) selectedIds.add(id); else selectedIds.delete(id);
        }
        cb.closest('tr')?.classList.toggle('rowSelected', want);
      });
      updateSelectionUI();
      syncSelectAllCheckbox();
    });
  }


  // Ultimate Skins filter
  const btnUlt = document.getElementById('btnUltimateSkins');
  const ultMenu = document.getElementById('ultimateMenu');
  if(btnUlt && ultMenu){
    btnUlt.addEventListener('click', (e)=>{
      e.preventDefault();
      e.stopPropagation();
      hideRareMenu();
      hideLegendaryMenu();
      toggleUltimateMenu();
    });

    ultMenu.querySelectorAll('button[data-min]').forEach(b=>{
      b.addEventListener('click', (e)=>{
        e.preventDefault();
        e.stopPropagation();
        const n = Number(b.getAttribute('data-min')) || 0;
        state.filters.ultimateMin = n;
        hideUltimateMenu();
        updateUltimateUI();
        render();
        saveState();
      });
    });

    document.addEventListener('click', (e)=>{
      if(ultMenu.classList.contains('hidden')) return;
      if(btnUlt.contains(e.target) || ultMenu.contains(e.target)) return;
      hideUltimateMenu();
    });

    window.addEventListener('resize', positionUltimateMenu);
    // capture scroll inside tableWrap too
    window.addEventListener('scroll', positionUltimateMenu, true);

    updateUltimateUI();
  }
  // Rare Skins filter
  const btnRare = document.getElementById('btnRareSkins');
  const rareMenu = document.getElementById('rareMenu');
  if(btnRare && rareMenu){
    btnRare.addEventListener('click', (e)=>{
      e.preventDefault();
      e.stopPropagation();
      // close ultimate menu if open
      hideUltimateMenu();
      hideLegendaryMenu();
      toggleRareMenu();
    });

    rareMenu.querySelectorAll('button[data-min]').forEach(b=>{
      b.addEventListener('click', (e)=>{
        e.preventDefault();
        e.stopPropagation();
        const n = Number(b.getAttribute('data-min')) || 0;
        state.filters.rareMin = n;
        hideRareMenu();
        updateRareUI();
        render();
        saveState();
      });
    });

    document.addEventListener('click', (e)=>{
      if(rareMenu.classList.contains('hidden')) return;
      if(btnRare.contains(e.target) || rareMenu.contains(e.target)) return;
      hideRareMenu();
    });

    window.addEventListener('resize', positionRareMenu);
    window.addEventListener('scroll', positionRareMenu, true);

    updateRareUI();
  }


  // Legendary Skins filter
  const btnLeg = document.getElementById('btnLegendarySkins');
  const legMenu = document.getElementById('legendaryMenu');
  const legWrap = document.getElementById('legendaryNums');
  if(btnLeg && legMenu && legWrap){
    // build number buttons dynamically based on max legendary skins in DATA
    buildLegendaryMenu();

    btnLeg.addEventListener('click', (e)=>{
      e.preventDefault();
      e.stopPropagation();
      // close other menus if open
      hideUltimateMenu();
      hideRareMenu();
      toggleLegendaryMenu();
    });

    legWrap.addEventListener('click', (e)=>{
      const b = e.target.closest('button[data-min]');
      if(!b) return;
      e.preventDefault();
      e.stopPropagation();
      const n = Number(b.getAttribute('data-min')) || 0;
      state.filters.legendaryMin = n;
      hideLegendaryMenu();
      updateLegendaryUI();
      render();
      saveState();
    });

    document.addEventListener('click', (e)=>{
      if(legMenu.classList.contains('hidden')) return;
      if(btnLeg.contains(e.target) || legMenu.contains(e.target)) return;
      hideLegendaryMenu();
    });

    window.addEventListener('resize', positionLegendaryMenu);
    window.addEventListener('scroll', positionLegendaryMenu, true);

    updateLegendaryUI();
  }


  document.getElementById('btnCloseModal').addEventListener('click', closeModal);
  document.getElementById('modal').addEventListener('click', (e)=>{
    if(e.target.id === 'modal') closeModal();
  });
  window.addEventListener('keydown', (e)=>{
    if(e.key === 'Escape') closeModal();
  });

  document.getElementById('btnCopyChamps').addEventListener('click', ()=>{
    if(!currentModalAccount) return;
    const t = (currentModalAccount.champions || []).slice().sort((x,y)=>x.localeCompare(y)).join('\n');
    copyText(t);
  });
  document.getElementById('btnCopySkins').addEventListener('click', ()=>{
    if(!currentModalAccount) return;
    const t = (currentModalAccount.skins || []).slice().sort((x,y)=>x.localeCompare(y)).join('\n');
    copyText(t);
  });
  document.getElementById('btnCopyLoot').addEventListener('click', ()=>{
    if(!currentModalAccount) return;
    const t = (currentModalAccount.loot_items || []).slice().join('\n');
    copyText(t);
  });

  document.querySelectorAll("thead th[data-key]").forEach(th => {
    const key = th.getAttribute("data-key");
    th.addEventListener("click", ()=>{
      if(state.sortKey === key) state.sortDir *= -1;
      else { state.sortKey = key; state.sortDir = 1; }
      render(); saveState();
    });
  });
}

function syncUIFromState(){
  document.getElementById('selRegion').value = state.filters.region;
  document.getElementById('selActivity').value = state.filters.activity;
  document.getElementById('inpSearch').value = state.filters.search;
  document.getElementById('inpChampion').value = '';
  document.getElementById('inpSkin').value = '';
  renderMultiTags();
  updateUltimateUI();
  updateRareUI();
  updateLegendaryUI();
}

(function main(){
  loadState();
  setTheme(state.theme);
  initSelects();
  initDatalists();
  hookControls();
  syncUIFromState();
  renderMultiTags();
  render();
})();
</script>
<canvas id="snow-canvas" aria-hidden="true"></canvas>


<script>
(() => {
  if (window.matchMedia && window.matchMedia('(prefers-reduced-motion: reduce)').matches) return;

  const canvas = document.getElementById('snow-canvas');
  if (!canvas) return;
  const ctx = canvas.getContext('2d', { alpha: true });

  let W = 0, H = 0, DPR = 1;
  function resize(){
    DPR = Math.max(1, Math.min(2, window.devicePixelRatio || 1));
    W = Math.floor(window.innerWidth);
    H = Math.floor(window.innerHeight);
    canvas.width = Math.floor(W * DPR);
    canvas.height = Math.floor(H * DPR);
    canvas.style.width = W + 'px';
    canvas.style.height = H + 'px';
    ctx.setTransform(DPR, 0, 0, DPR, 0, 0);
  }

  const flakes = [];
  function rand(min, max){ return Math.random() * (max - min) + min; }

  function targetCount(){
    // scale gently with screen area
    const area = (window.innerWidth * window.innerHeight) / (1920 * 1080);
    return Math.max(90, Math.min(220, Math.round(140 * area)));
  }

  function makeFlake(spawnTop=false){
    const r = rand(0.8, 3.2);
    return {
      x: rand(0, W),
      y: spawnTop ? rand(-H, 0) : rand(0, H),
      r,
      v: rand(0.35, 1.4) * (0.7 + (r/3.2)*0.9),
      drift: rand(-0.22, 0.22),
      wobble: rand(0, Math.PI*2),
      wobbleSpeed: rand(0.002, 0.010),
      alpha: rand(0.35, 0.95)
    };
  }

  function resetFlakes(){
    flakes.length = 0;
    const n = targetCount();
    for(let i=0;i<n;i++) flakes.push(makeFlake(true));
  }

  let last = performance.now();
  let running = true;

  function tick(now){
    if(!running) return;
    const dt = Math.min(32, now - last);
    last = now;

    // background clear
    ctx.clearRect(0, 0, W, H);

    // subtle glow for nicer snow
    ctx.save();
    ctx.globalCompositeOperation = 'lighter';

    for(const f of flakes){
      f.wobble += f.wobbleSpeed * dt;
      const wob = Math.sin(f.wobble) * 0.6;
      f.x += (f.drift + wob) * dt * 0.04;
      f.y += f.v * dt * 0.05;

      if(f.y - f.r > H){
        // respawn at top
        f.x = rand(0, W);
        f.y = rand(-20, -2);
        f.r = rand(0.8, 3.2);
        f.v = rand(0.35, 1.4) * (0.7 + (f.r/3.2)*0.9);
        f.drift = rand(-0.22, 0.22);
        f.alpha = rand(0.35, 0.95);
      }
      if(f.x < -10) f.x = W + 10;
      if(f.x > W + 10) f.x = -10;

      // draw flake
      ctx.beginPath();
      ctx.fillStyle = `rgba(255,255,255,${f.alpha})`;
      ctx.arc(f.x, f.y, f.r, 0, Math.PI * 2);
      ctx.fill();

      // tiny highlight
      ctx.beginPath();
      ctx.fillStyle = `rgba(255,255,255,${Math.min(1, f.alpha + 0.25)})`;
      ctx.arc(f.x - f.r*0.25, f.y - f.r*0.25, f.r*0.35, 0, Math.PI*2);
      ctx.fill();
    }

    ctx.restore();
    requestAnimationFrame(tick);
  }

  // start
  resize();
  resetFlakes();
  requestAnimationFrame(tick);

  // keep stable on resize
  let resizeTimer;
  window.addEventListener('resize', () => {
    clearTimeout(resizeTimer);
    resizeTimer = setTimeout(() => {
      resize();
      resetFlakes();
    }, 120);
  });

  document.addEventListener('visibilitychange', () => {
    running = !document.hidden;
    if(running){
      last = performance.now();
      requestAnimationFrame(tick);
    }
  });
})();
</script>

</body>
</html>
