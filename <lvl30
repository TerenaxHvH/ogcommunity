// Fixed reference date: checked on 14/01/2026
const LAST_CHECKED = new Date(2026, 0, 14);

const LS = {
  theme: "lol_customer_theme",
  themeV3: "lol_customer_theme_v3",
  rankedReady: "lol_customer_ranked_ready",
  filters: "lol_customer_filters_v2",
  sort: "lol_customer_sort_v2"
};

const state = {
  theme: "light",
  rankedReadyOnly: false,
  sortKey: "",
  sortDir: 1,
  filters: {
    region: "All",
    activity: "All",
    search: "",
    championsSelected: [],
    skinsSelected: [],
    ultimateMin: 0,
    rareMin: 0,
    legendaryMin: 0,
    // legacy single-text filters (kept for backward compatibility)
    champion: "",
    skin: ""
  }
};

const nf = new Intl.NumberFormat();
const fmtNum = (v) => nf.format(Number(v)||0);

function loadState(){
  const theme = localStorage.getItem(LS.theme);
  const themeV3 = localStorage.getItem(LS.themeV3);
  if(theme){
    // Backward compat:
    // - Older versions stored: "night" | "day"
    // - New version stores: "light" | "day" | "night"
    if(theme === "night" && !themeV3) state.theme = "light";
    else state.theme = theme;
  }

  const rr = localStorage.getItem(LS.rankedReady);
  if(rr !== null) state.rankedReadyOnly = (rr === "1");

  const f = localStorage.getItem(LS.filters);
  if(f){
    try{
      const obj = JSON.parse(f);
      state.filters = {...state.filters, ...obj};
      // cleanup removed options
      delete state.filters.championMode;
      delete state.filters.skinMode;

      // ensure multi-select arrays exist
      if(typeof state.filters.ultimateMin !== 'number') state.filters.ultimateMin = 0;
      if(typeof state.filters.rareMin !== 'number') state.filters.rareMin = 0;
      if(typeof state.filters.legendaryMin !== 'number') state.filters.legendaryMin = 0;
      if(!Array.isArray(state.filters.championsSelected)) state.filters.championsSelected = [];
      if(!Array.isArray(state.filters.skinsSelected)) state.filters.skinsSelected = [];

      // migrate legacy single inputs into chips
      if(typeof state.filters.champion === 'string' && state.filters.champion.trim()){
        const v = state.filters.champion.trim();
        if(!state.filters.championsSelected.some(x => norm(x) === norm(v))) state.filters.championsSelected.push(v);
        state.filters.champion = "";
      }
      if(typeof state.filters.skin === 'string' && state.filters.skin.trim()){
        const v = state.filters.skin.trim();
        if(!state.filters.skinsSelected.some(x => norm(x) === norm(v))) state.filters.skinsSelected.push(v);
        state.filters.skin = "";
      }
    }catch{}
  }

  const s = localStorage.getItem(LS.sort);
  if(s){
    try{
      const obj = JSON.parse(s);
      if(obj.sortKey) state.sortKey = obj.sortKey;
      if(state.sortKey === 'ranked_ready'){ state.sortKey = ''; state.sortDir = 1; }
      if(obj.sortDir) state.sortDir = obj.sortDir;
    }catch{}
  }
}

function saveState(){
  localStorage.setItem(LS.theme, state.theme);
  localStorage.setItem(LS.themeV3, "1");
  localStorage.setItem(LS.rankedReady, state.rankedReadyOnly ? "1" : "0");
  localStorage.setItem(LS.filters, JSON.stringify(state.filters));
  localStorage.setItem(LS.sort, JSON.stringify({sortKey: state.sortKey, sortDir: state.sortDir}));
}

const THEME_ORDER = ["light","day","night"];
const THEME_LABEL = {light:"Light mode", day:"Day mode", night:"Night mode"};

function setTheme(theme){
  if(!THEME_LABEL[theme]) theme = "light";
  state.theme = theme;
  document.body.classList.toggle("day", theme === "day");
  document.body.classList.toggle("night", theme === "night");
  document.body.classList.toggle("light", theme === "light");
  document.getElementById("btnTheme").textContent = THEME_LABEL[theme];
  // Night mode should exactly match V3 styling
  const styleMain = document.getElementById("styleMain");
  const styleNight = document.getElementById("styleNight");
  if(styleMain && styleNight){
    if(theme === "night"){
      styleNight.media = "all";
      styleMain.media = "not all";
    }else{
      styleNight.media = "not all";
      styleMain.media = "all";
    }
  }
  localStorage.setItem(LS.themeV3, "1");
  saveState();
}

function toggleTheme(){
  const i = THEME_ORDER.indexOf(state.theme);
  const next = THEME_ORDER[( (i < 0 ? 0 : i) + 1 ) % THEME_ORDER.length];
  setTheme(next);
}

function norm(s){ return (s ?? "").toString().trim().toLowerCase(); }

function parseDMY(s){
  const m = /^(\d{2})\/(\d{2})\/(\d{4})$/.exec((s||"").trim());
  if(!m) return null;
  const dd = parseInt(m[1],10);
  const mm = parseInt(m[2],10);
  const yy = parseInt(m[3],10);
  return new Date(yy, mm-1, dd);
}

